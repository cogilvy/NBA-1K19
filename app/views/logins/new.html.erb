<h1>Login To Existing Account</h1>

<%= form_tag login_path do %>
  <ul>
    <li>
      <%= label_tag :username %>
      <%= text_field_tag :username %>
    </li>
    <li>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </li>
  </ul>
  <%= submit_tag "Login"%>
<% end %>

<%= link_to "Back To Home", root_path %>
